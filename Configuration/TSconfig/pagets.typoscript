mod.wizards.newContentElement.wizardItems.plugins.elements {
    blogging_postlistplugin {
        iconIdentifier = tx_blogging_icon
        title = LLL:EXT:blogging/Resources/Private/Language/locallang_be.xlf:tx_blogging_plugin_postlist
        description = LLL:EXT:blogging/Resources/Private/Language/locallang_be.xlf:tx_blogging_plugin_postlist_description
        tt_content_defValues {
            CType = list
            list_type = blogging_postlistplugin
        }
    }

    blogging_categorypostlistplugin < .blogging_postlistplugin
    blogging_categorypostlistplugin.title = LLL:EXT:blogging/Resources/Private/Language/locallang_be.xlf:tx_blogging_plugin_categorypostlist
    blogging_categorypostlistplugin.description = LLL:EXT:blogging/Resources/Private/Language/locallang_be.xlf:tx_blogging_plugin_categorypostlist_description
    blogging_categorypostlistplugin.tt_content_defValues.list_type = blogging_categorypostlistplugin
}